{
  "name": "email",
  "strategy": "email",
  "is_domain_connection": false,
  "enabled_clients": ["Auth0 Demo SPA"],
  "options": {
    "disable_signup": false,
    "name": "email",
    "email": {
      "syntax": "liquid",
      "from": "{{ application.name }} <rdahbura@gmail.com>",
      "subject": "Welcome to {{ application.name }}",
      "body": "./email.html"
    },
    "authParams": "",
    "totp": {
      "time_step": 300,
      "length": 6
    },
    "brute_force_protection": true
  }
}
