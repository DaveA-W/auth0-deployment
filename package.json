{
  "name": "auth0-deployment",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Auth0 custom database, hosted pages and rules scripts.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdahbura/auth0-deployment.git"
  },
  "scripts": {
    "format": "prettier --write './{clients,connections,database-connections,emails,grants,pages,resource-servers,roles,rules}/**/*.{css,html,js,json,jsx}'"
  },
  "dependencies": {
    "lodash": "^4.17.14",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-html": "^6.0.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "plugins": [
      "html"
    ]
  }
}
